{
  "name": "cityrow",
  "private": true,
  "description": "website and small ecommerce",
  "version": "2.5.1",
  "license": "0BSD",
  "scripts": {
    "build": "npm run clean && gatsby build --verbose",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint .",
    "lint-modified-files": "eslint $(git diff --name-only --diff-filter=db $(git merge-base HEAD $BITBUCKET_PR_DESTINATION_BRANCH) '**/*.js' '**/*.ts' '**/*.tsx')",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "npx jest --coverage && jest-it-up",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "eslint",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.15.0",
    "@builder.io/gatsby": "^3.0.0",
    "@builder.io/react": "^2.0.10",
    "@builder.io/widgets": "^1.2.23",
    "@material-ui/core": "^4.11.4",
    "@segment/analytics-next": "^1.38.0",
    "@stripe/stripe-js": "^1.22.0",
    "@types/classnames": "^2.2.11",
    "@types/react-slick": "^0.23.4",
    "axios": "^1.13.2",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "gatsby": "^5.15.0",
    "gatsby-plugin-less": "^7.15.0",
    "gatsby-plugin-meta-redirect": "^1.1.1",
    "gatsby-plugin-rollbar": "^1.1.0",
    "gatsby-plugin-sass": "^6.15.0",
    "gatsby-plugin-typescript": "^5.15.0",
    "js-sha1": "^0.6.0",
    "less": "^4.1.1",
    "lodash": "^4.17.21",
    "react": "^17.0.1",
    "react-content-loader": "^6.0.3",
    "react-dom": "^17.0.1",
    "react-google-places-autocomplete": "^3.3.2",
    "react-scroll-parallax": "^3.0.5",
    "react-slick": "^0.29.0",
    "sass": "^1.97.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.1",
    "slick-carousel": "^1.8.1",
    "source-map-support": "^0.5.19",
    "swell-js": "^3.17.1",
    "ts-custom-error": "^3.2.0",
    "ts-node": "^9.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.4.0",
    "gatsby-plugin-resolve-src": "^2.1.0",
    "husky": "^7.0.0",
    "jest": "^27.3.1",
    "jest-it-up": "^2.0.0",
    "lint-staged": "^12.3.4",
    "prettier": "^2.3.1",
    "ts-jest": "^27.0.7",
    "ts-mockito": "^2.6.1",
    "typescript": "^4.4.4"
  }
}
